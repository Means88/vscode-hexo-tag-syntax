{
  "name": "vscode-hexo-tag-syntax",
  "displayName": "Hexo Tag Syntax",
  "description": "",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "publisher": "means88",
  "repository": {
    "type": "git",
    "url": "https://github.com/Means88/vscode-hexo-tag-syntax"
  },
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "hexo-ts-injection"
      }
    ],
    "grammars": [
      {
        "language": "hexo-ts-injection",
        "scopeName": "markdown.hexo-ts.codeblock",
        "path": "./syntaxes/hexo-tag-md.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.hexo-js": "javascript",
          "meta.embedded.block.hexo-jsx": "javascriptreact",
          "meta.embedded.block.hexo-ts": "typescript",
          "meta.embedded.block.hexo-tsx": "typescriptreact",
          "meta.embedded.block.hexo-json": "json",
          "meta.embedded.block.hexo-jsonc": "jsonc"
        }
      }
    ]
  }
}
